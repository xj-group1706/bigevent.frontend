{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "include": ["generate-i18n-files.ts"],
  "compilerOptions": {
    "types": [
      "node",
      "@nuxtjs/i18n",
      "./types/i18n.d.ts",
      "@sidebase/nuxt-auth"
    ],
    "paths": {
      "#i18n": ["./node_modules/@nuxtjs/i18n/dist/runtime/composables"],
      "@/*": ["./*"]
    }
  }
}
